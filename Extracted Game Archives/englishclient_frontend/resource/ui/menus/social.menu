        ScreenFrame
        {
            ControlName				Label
            xpos					0
            ypos					0
            wide					%100
            tall					%100
            labelText				""
            //visible				    1
            //bgcolor_override        "255 255 0 100"
            //paintbackground         1
        }

        ScreenBlur
        {
            ControlName				Label
            labelText               ""
        }

        ToolTip
        {
            ControlName				RuiPanel
            InheritProperties       ToolTip
            zpos                    999
        }

		FooterButtons
		{
			ControlName				CNestedPanel
			InheritProperties       FooterButtons
		}

        Vignette
        {
            ControlName             ImagePanel
            InheritProperties       MenuVignette
        }

        MatchmakingStatus
        {
            ControlName		        RuiPanel
            InheritProperties		MatchmakingStatus

            pin_to_sibling			ScreenFrame
            pin_corner_to_sibling	BOTTOM
            pin_to_sibling_corner	BOTTOM
        }

        MenuHeader
        {
            ControlName				RuiPanel
			ypos					-48
            wide					1800
            tall					208
            visible				    1
            rui                     "ui/friend_menu_header.rpak"

            pin_to_sibling			ScreenFrame
            pin_corner_to_sibling	TOP
            pin_to_sibling_corner	TOP
        }

		Decoration
		{
			ControlName             RuiPanel
   			wide					f0
   			tall                    f0
	        rui                     "ui/customize_character_menu_decoration.rpak"
		}

        Logo
        {
            ControlName				RuiPanel
			InheritProperties       Logo

            pin_to_sibling			ScreenFrame
            pin_corner_to_sibling	TOP_LEFT
            pin_to_sibling_corner	TOP_LEFT
        }

		PanelFrame
		{
            ControlName				Label
            xpos					0
            ypos					0
            wide					1800
            tall					660
            labelText				""
            visible				    0
            bgcolor_override        "255 255 0 100"
            paintbackground         1

            pin_to_sibling          MenuHeader
            pin_corner_to_sibling   TOP
            pin_to_sibling_corner   BOTTOM
		}

		MyGridButton
		{
            ControlName			    RuiButton
            InheritProperties	    FriendGridButton
            visible			        1
            tabPosition             1

            navDown                 PartyPrivacyButton
            navRight                GridButton0x0

            pin_to_sibling          PanelFrame
            pin_corner_to_sibling   TOP_LEFT
            pin_to_sibling_corner   TOP_LEFT
		}

        PartyPrivacyButton
        {
            ControlName             RuiButton
            classname               "MenuButton"
            wide                    400
            tall                    80
            xpos                    0
            ypos                    16
            rui                     "ui/generic_left_icon_button.rpak"
            labelText               ""
            visible                 1
            cursorVelocityModifier  0.7

            navUp                   MyGridButton
            navRight                GridButton1x0

            pin_to_sibling          MyGridButton
            pin_corner_to_sibling   TOP_RIGHT
            pin_to_sibling_corner   BOTTOM_RIGHT
        }

        LeavePartyButton
        {
            ControlName             RuiButton
            classname               "MenuButton"
            wide                    400
            tall                    80
            ypos                    16
            rui                     "ui/generic_left_icon_button.rpak"
            labelText               ""
            visible                 1
            cursorVelocityModifier  0.7

            navUp                   PartyPrivacyButton
            navDown                 SteamLink [!$GAMECONSOLE]
            navRight                GridButton2x0

            pin_to_sibling          PartyPrivacyButton
            pin_corner_to_sibling   TOP_RIGHT
            pin_to_sibling_corner   BOTTOM_RIGHT
        }

        AddFriendButton
        {
            ControlName             RuiButton
            classname               "MenuButton"
            wide                    400
            tall                    80
            ypos                    0//96//16
            rui                     "ui/generic_left_icon_button.rpak"
            labelText               ""
            visible                 0
            cursorVelocityModifier  0.7

            pin_to_sibling			PanelFrame
            pin_corner_to_sibling	BOTTOM_LEFT
            pin_to_sibling_corner	BOTTOM_LEFT
        }

        SteamLink [!$GAMECONSOLE]
        {
            ControlName				RuiButton
            classname               "MenuButton"
            ypos					16
            wide					400
            tall					80
            visible					1

            navUp                   LeavePartyButton
