
"UnlitTwoTexture"
{
	"$nodecal" "1"
	$allowoverbright 1
	"$vertcoloristint" "1"

	"$godray" "0"
	"$godrayMinFadeDist" "100"
	"$godrayMaxFadeDist" "10000"

	"$additive" "1"
	"$vertexcolor" 1
	"$vertexalpha" 1
	"$translucent" 1
	"$nocull" "1"
	"$color2" "[1 1 1]"


	"$basetexture" "effects\tvscreen_noise001b"
	//"$basetexture" "effects\caustic_water_01_alpha"
	"$Texture2" "dev\noise_post"


	"$nodecal" 1

	$fresnel 1
	$fresnelsharpness1 1
	$fresnelouterstrength1 2
	$fresnelinnerstrength1 1

	$layeralpha1 "0.1"
	$layeralpha2 "1"
	//$addlayers 1


	$edgeDetection "1"				//Enable Edge Detect
	$edge.silhouette "1" 			//Generate silhouette edges
	$edge.depth "1" 				//Generate depth edges
	$edge.outlineColor "[1 1 1]"	//Outline Color
	$edge.outlineOpacity "1"		//Outline opacity
	$edge.outlineWidth "10.0"		//Outline width in world units
	$edge.outlineFalloffExp "5.0"	//Outline border fall-off exponent
	$edge.fadeInStart "20.0"		//Fade-in start distance from the camera
	$edge.fadeInEnd "30.0"			//Fade-in end distance from the camera
	$edge.fadeOutStart "7000.0"		//Fade-out start distance from the camera
	$edge.fadeOutEnd "8000.0"		//Fade-out end distance from the camera



	$screenspacecoordssquare1 0
	$screenspacecoordssquare2 0


	"Proxies"
	{
		"AnimatedTexture"
  		{
			"animatedtexturevar" "$texture2"
			"animatedtextureframenumvar" "$frame2"
			"animatedtextureframerate" "30"
		}

		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 3
			"texturescrollangle" -90
			"texturescale"	3
		}

		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" 1
			"texturescrollangle" 0
			"texturescale"	5
		}
		//flicker Edge Dectect
        "UniformNoise"
        {
            "minVal" 5
            "maxVal" 7
            "resultVar" "$edge.outlineFalloffExp"
        }

	}
}
