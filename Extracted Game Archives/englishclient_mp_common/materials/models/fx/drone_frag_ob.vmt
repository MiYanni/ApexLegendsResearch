"UnlitTwoTexture"
{

	"$basetexture" "models\vehicle\straton\gunship_afterburner"
	"$texture2" "effects\laserplane_atmosphere"

	$allowoverbright 1

	$decal 1

//	"$nocull"      1
	"$translucent" 1
	"$vertexalpha" 1

	"$additive" 1

	$layercolor1 "[3 3 3]"
	$layeralpha1 "1"

	$layercolor2 "[3 3 3]"
	$layeralpha2 "1"


	//Proxy Variables
	//$one "1.0"
	//$ClipAmmoFraction "0.0"

	"Proxies"
	{

		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" "-0.05"
			"texturescrollangle" "180"
			"texturescale"	2
		}


	}

}
