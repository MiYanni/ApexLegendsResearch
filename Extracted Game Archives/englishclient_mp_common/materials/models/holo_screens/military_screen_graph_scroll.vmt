"UnlitTwoTexture"
{
	"$baseTexture" "models/holo_screens/military_screen_graph_scroll_col"
	"$texture2" "effects/dev_scanline"
	"$additive" 1
	"$translucent" 1
	"$color" "[1 1 1]"
	"Proxies"
	{
		
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 1
